{
  "name": "authorizations-client",
  "version": "0.1.0",
  "description": "AuthorizationsClient",
  "productName": "AuthorizationsClient",
  "author": "https://github.com/Virus288",
  "license": "Apache-2.0",
  "private": false,
  "type": "module",
  "scripts": {
    "start:dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "cross-env NODE_ENV=development eslint -c eslint.config.mjs . --fix",
    "lintStaged": "npx lint-staged",
    "listErrors": "node_modules/.bin/tsc --skipLibCheck"
  },
  "lint-staged": {
    "*.ts": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-icons": "^5.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
